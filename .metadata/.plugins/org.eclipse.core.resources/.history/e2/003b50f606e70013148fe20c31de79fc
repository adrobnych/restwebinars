package com.sysiq.restwebinars.user_client_spec;


import org.junit.BeforeClass;
import org.junit.Test;


import static org.junit.Assert.*;


public class UserExchangeDispatcherSpec{

	private static UserExchangeDispatcherSpec ed = null;
	
	@BeforeClass
	public static void init(){
		ed = ExchangeDispatcher.createInstance();
	}

	@Test
	public void testSetRate() {
		ed.setRate(12.01);
		assertEquals(12.01, ed.getRate(), 0.0001);
	}

}